            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "train_luna_classification",
                "0.0.0-profile", // model version
                "--epochs=10",
                "--batch-size=128",
                "--profile",
            ],
            "justMyCode": false,
        },
