    "version": "0.2.0",
    "configurations": [
        {
            "name": "LuNA Classification Testing",
            "consoleTitle": "LuNA Classification Testing",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/luna16/cli.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "train_luna_classification",
                "0.0.0-test",
                "--epochs=2",
                "--batch-size=32",
                "--profile",
            ],
            "justMyCode": false
        },
        {
            "name": "LuNA Classification Training",
            "consoleTitle": "LuNA Classification Training",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/luna16/cli.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "."
            },
            "args": [
                "train_luna_classification",
                "0.0.0",
                "--epochs=10",
                "--batch-size=64",
            ]
        },
        {
            "name": "Profiling LuNA Classification",
            "consoleTitle": "Profiling LuNA Classification",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/luna16/cli.py",
